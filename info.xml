<?xml version="1.0"?>
<extension key="de.systopia.civioffice" type="module">
  <file>civioffice</file>
  <name>CiviOffice</name>
  <description>CiviCRM integration for various exchangeable office suites</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>franz@systopia.de, endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.civioffice</url>
    <url desc="Documentation">https://docs.civicrm.org/civioffice/en/latest</url>
    <url desc="Support">https://github.com/systopia/de.systopia.civioffice/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>1.1.0-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.76</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Civioffice</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.0</mixin>
    <mixin>ang-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Civioffice_Upgrader</upgrader>
</extension>
